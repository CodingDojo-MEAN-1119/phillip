        <p><span>Hiding Spots: </span>
            <%for (let i=0; i<cat.h_spots.length; i++){%>
                    <%=cat.h_spots[i]%>, 
            <%}%>
        </p>
        <br>
        <a href="/">Back to Home</a>  |  <a href="/cats/edit/<%=cat.id%>">Edit </a>  |  <a href="/cats/destroy/<%=cat.id%>">Delete</a>
    </div>
</body>
</html>